	private int id;                  // id area
	private String name;             // name area
	private String coordinates;	     // coordinates area
	private Polygon polygon;         // сама область, постороенная по координатам
	
	private RegionWesteros(int id, String name, String coordinates) {
		this.id = id;
		this.name = name;
		this.coordinates = coordinates;
		polygon = new Polygon();
		ArrayList<Double> arrayCoordinates = new ArrayList<Double>();
		String[] array = coordinates.split(",");  		
		for (String s : array) {   
			arrayCoordinates.add(Double.parseDouble(s));
		}		
		polygon.getPoints().addAll(arrayCoordinates);    // строим область по координатам
		polygon.setOpacity(0);                           // сразу делаем ее прозрачной                        
		polygon.setFill(Color.RED);                      // пусть она красного цвета будет
	}

	public int getId() {
		return id;
	}
	
	public String getName() {
		return name;
	}

	public String getCoordinates() {
		return coordinates;
	}

	public Polygon getPolygon() {
		return polygon;
	}
	
	public void printCoord() {
		int i = 1;
		for (Double d : polygon.getPoints()) {
			if (i%2 == 1) System.out.print(d + ",");
			else {
				System.out.print(" " + d);
				System.out.println();
			}
			i++;
		}
	}
	
}
